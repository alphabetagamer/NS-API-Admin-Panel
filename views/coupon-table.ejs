                        <div class="card">
                            <h5 class="card-header">Coupons</h5>
                            <div class="card-body p-0">
                                <div class="table-responsive">
                                    <table class="table">
                                        <thead class="bg-light">
                                            <tr class="border-0">
                                                <th class="border-0">#</th>
                                                <th class="border-0">image</th>
                                                <th class="border-0">User Name</th>
                                                <th class="border-0">Refered by username</th>
                                                <th class="border-0">Phone</th>
                                                <th class="border-0">Amount Earned</th>
                                                
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for(var i=0; i < user.length; i++) { 
                                                console.log(user[i].profile_image)
                                                %>
                                                  <tr>
                                                <td><%= i+1 %></td>
                                                <td>
                                                    <div class="m-r-10"><img src="<%= user[i].profile_image %>"  alt="user" class="rounded" width="45"></div>
                                                </td>
                                                <td><a href="<%= 'single-user?uname='+user[i].username %>"><%= user[i].name.first_name %> <% user[i].name.last_name %></a></td>
                                                 <td><%= refer_i[i]%></td>
                                                 <td><%= user[i].mobile %></td>
                                                 <td contentEditable="true">150</td>
                                                
                                               
                                            </tr>
                                            <% } %>
                                                
                                            
                                           

                                        </tbody>
                                    </table>
