<% include ./partials/header %>
        <!-- ============================================================== -->
        <!-- end left sidebar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- wrapper  -->
        <!-- ============================================================== -->
 <!-- wrapper  -->
        <!-- ============================================================== -->
        <div class="dashboard-wrapper">
            <div class="container-fluid  dashboard-content">
                <!-- ============================================================== -->
                <!-- pagehader  -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="page-header">
                            <h3 class="mb-2">Product Panel</h3> 
                            <a class="btn btn btn-info" href="/add-review" style="float: right;">Reviews</a>
                            <p class="pageheader-text">Lorem ipsum dolor sit ametllam fermentum ipsum eu porta consectetur adipiscing elit.Nullam vehicula nulla ut egestas rhoncus.</p>
                            <div class="page-breadcrumb">
                                <nav aria-label="breadcrumb">
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">Dashboard</a></li>
                                        <li class="breadcrumb-item active" aria-current="page">Review module</li>
                                    </ol>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- ============================================================== -->
                <!-- pagehader  -->
                <!-- ============================================================== -->
           
                <!-- ============================================================== -->
                <!-- revenue  -->
                <!-- ============================================================== -->
            



           




          


                <div class="row">
                    <!-- ============================================================== -->
                    <!-- top selling products  -->
                    <!-- ============================================================== -->
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="card">
                            <h5 class="card-header">Reviews Table</h5>
                            <div class="card-body p-0">
                                <div class="table-responsive">
                                    <table class="table">
                                        <thead class="bg-light">
                                            <tr class="border-0">
                                                <th class="border-0">#</th>
                                                <!-- <th class="border-0">image</th> --> 
                                                <th class="border-0">Customer Name</th>
                                                <th class="border-0">Product</th>
                                                <th class="border-0">Rating</th>
                                                <th class="border-0">Review</th>
                                                 <th class="border-0">Time</th>
                                                <th class="border-0">Edit</th>
                                                <th class="border-0">Delete</th>
                                               

                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for (var i =0 ; i< reviews.length ; i++) {
                                             %>
                                                  <tr>
                                                <td contentEditable="false"><%= i+1 %></td>
                                                <!-- <td> -->
                                                    <!-- <div class="m-r-10"><img    alt="user" class="rounded" width="45"></div> -->
                                                <!-- </td> -->
                                                <td contentEditable="false"><a><%= reviews[i].username %></a></td>
                                                <td contentEditable="false"><%= prod[i].name %></td>
                                                 <td contentEditable="false">
                                                     <% for(var k = 0 ; k< reviews[i].rating ; k ++){ %>
                                                     <span class="fa fa-star checked"></span>
                                                    <% } %>
                                                    <% for(var k = 0 ; k< 5-reviews[i].rating ; k ++) {%>
                                                    <span class="fa fa-star"></span>
                                                    <% } %>

                                                 </td>
                                                 <td contentEditable="false"><%= reviews[i].review_text %></td>
                                                 <td contentEditable="false"><%= reviews[i].date %></td>
                                                <td><a href='<%="/add-review?review="+reviews[i].id %>' class="btn btn-dark btn-sm">Edit</a></td>
                                                <td><a   class="btn btn-danger btn-sm">Delete</a></td>
                                               
                                            </tr>
                                            <% } %>
                                                 
                                            <tr>
                                                <td colspan="12"><a href="#" class="btn btn-outline-light float-right">View Details</a></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- ============================================================== -->
                    <!-- end top selling products  -->
                    <!-- ============================================================== -->
    <script src="assets/vendor/jquery/jquery-3.3.1.min.js"></script>
    <!-- bootstrap bundle js-->
    <script src="assets/vendor/bootstrap/js/bootstrap.bundle.js"></script>
    <!-- slimscroll js-->
    <script src="assets/vendor/slimscroll/jquery.slimscroll.js"></script>
    <!-- chartjs js-->
    <script src="assets/vendor/charts/charts-bundle/Chart.bundle.js"></script>
    <script src="assets/vendor/charts/charts-bundle/chartjs.js"></script>
   
    <!-- main js-->
    <script src="assets/libs/js/main-js.js"></script>
    <!-- jvactormap js-->
    <script src="assets/vendor/jvectormap/jquery-jvectormap-2.0.2.min.js"></script>
    <script src="assets/vendor/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- sparkline js-->
    <script src="assets/vendor/charts/sparkline/jquery.sparkline.js"></script>
    <script src="assets/vendor/charts/sparkline/spark-js.js"></script>
     <!-- dashboard sales js-->
    <script src="assets/libs/js/dashboard-sales.js"></script>
</body>
 
</html>